   "source": [
    "## üè¢ Publisher Performance Analysis\n",
    "\n",
    "**Key Discovery:** Specialization beats volume - Vertigo achieves 90% hit rate vs Penguin's 36%"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Publisher Efficiency Analysis\n",
    "publisher_query = \"\"\"\n",
    "SELECT \n",
    "    p.publisher,\n",
    "    COUNT(b.book_id) as volumen_libros,\n",
    "    ROUND(AVG(rating_data.avg_rating), 2) as calidad_promedio,\n",
    "    COUNT(CASE WHEN rating_data.avg_rating >= 4.0 THEN 1 END) as hits_alta_calidad,\n",
    "    ROUND(COUNT(CASE WHEN rating_data.avg_rating >= 4.0 THEN 1 END) * 100.0 / COUNT(b.book_id), 1) as efficiency_score,\n",
    "    CASE \n",
    "        WHEN COUNT(b.book_id) >= 30 AND ROUND(COUNT(CASE WHEN rating_data.avg_rating >= 4.0 THEN 1 END) * 100.0 / COUNT(b.book_id), 1) < 50 THEN 'Volume Strategy'\n",
    "        WHEN COUNT(b.book_id) <= 15 AND ROUND(COUNT(CASE WHEN rating_data.avg_rating >= 4.0 THEN 1 END) * 100.0 / COUNT(b.book_id), 1) >= 80 THEN 'Premium Specialist'\n",
    "        WHEN ROUND(COUNT(CASE WHEN rating_data.avg_rating >= 4.0 THEN 1 END) * 100.0 / COUNT(b.book_id), 1) >= 60 THEN 'Quality Focus'\n",
    "        ELSE 'Balanced Approach'\n",
    "    END as strategy_type\n",
    "FROM publishers p\n",
    "JOIN books b ON p.publisher_id = b.publisher_id\n",
    "LEFT JOIN (\n",
    "    SELECT book_id, AVG(rating) as avg_rating\n",
    "    FROM ratings \n",
    "    GROUP BY book_id\n",
    ") rating_data ON b.book_id = rating_data.book_id\n",
    "WHERE rating_data.avg_rating IS NOT NULL\n",
    "GROUP BY p.publisher_id, p.publisher\n",
    "HAVING COUNT(b.book_id) >= 8\n",
    "ORDER BY efficiency_score DESC, calidad_promedio DESC\n",
    "LIMIT 12;\n",
    "\"\"\"\n",
    "\n",
    "publisher_data = pd.read_sql(publisher_query, engine)\n",
    "\n",
    "# Visualization: Publisher Efficiency\n",
    "fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(18, 8))\n",
    "fig.suptitle('üè¢ PUBLISHER PERFORMANCE: Specialization vs Volume Strategy', fontsize=18, fontweight='bold')\n",
    "\n",
    "# Efficiency Ranking\n",
    "top_publishers = publisher_data.head(8)\n",
    "colors_efficiency = plt.cm.RdYlGn([x/100 for x in top_publishers['efficiency_score']])\n",
    "\n",
    "bars = ax1.barh(range(len(top_publishers)), top_publishers['efficiency_score'], \n",
    "                color=colors_efficiency, alpha=0.8)\n",
    "\n",
    "ax1.set_yticks(range(len(top_publishers)))\n",
    "ax1.set_yticklabels(top_publishers['publisher'], fontsize=10)\n",
    "ax1.set_title('üìä Publisher Efficiency Score (% of High-Quality Books)', fontsize=12, fontweight='bold')\n",
    "ax1.set_xlabel('Efficiency Score (%)')\n",
    "\n",
    "# Add efficiency values\n",
    "for i, value in enumerate(top_publishers['efficiency_score']):\n",
    "    ax1.text(value + 1, i, f'{value}%', va='center', fontweight='bold')\n",
    "\n",
    "# Volume vs Quality Scatter\n",
    "scatter = ax2.scatter(publisher_data['volumen_libros'], publisher_data['efficiency_score'], \n",
    "                     s=[x*15 for x in publisher_data['volumen_libros']], \n",
    "                     c=publisher_data['calidad_promedio'], cmap='viridis', \n",
    "                     alpha=0.7, edgecolors='black')\n",
    "\n",
    "ax2.set_xlabel('Volume (Number of Books)', fontsize=12)\n",
    "ax2.set_ylabel('Efficiency Score (%)', fontsize=12)\n",
    "ax2.set_title('üìà Volume vs Quality Strategy', fontsize=12, fontweight='bold')\n",
    "\n",
    "# Add colorbar\n",
    "cbar = plt.colorbar(scatter, ax=ax2)\n",
    "cbar.set_label('Average Quality Rating', fontsize=10)\n",
    "\n",
    "# Annotate key players\n",
    "for idx, row in top_publishers.head(3).iterrows():\n",
    "    ax2.annotate(row['publisher'], \n",
    "                (row['volumen_libros'], row['efficiency_score']),\n",
    "                xytext=(5, 5), textcoords='offset points',\n",
    "                fontsize=9, fontweight='bold')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(\"üí° PUBLISHER INSIGHTS:\")\n",
    "print(\"=\" * 50)\n",
    "print(f\"ü•á Top Performer: {publisher_data.iloc[0]['publisher']} ({publisher_data.iloc[0]['efficiency_score']}% efficiency)\")\n",
    "print(f\"üìä Strategy: {publisher_data.iloc[0]['strategy_type']}\")\n",
    "print(f\"üìö Portfolio: {publisher_data.iloc[0]['volumen_libros']} books, {publisher_data.iloc[0]['hits_alta_calidad']} hits\")\n",
    "print(\"\\nüîç Key Finding: Specialization consistently outperforms volume strategy\")"
   ]
  },
  {
