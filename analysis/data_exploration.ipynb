   "source": [
    "## üë• User Engagement Hierarchy\n",
    "\n",
    "**Critical Discovery:** Power Users (0.8%) generate 15x more value than average users"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# User Segmentation Analysis\n",
    "user_query = \"\"\"\n",
    "WITH user_stats AS (\n",
    "    SELECT \n",
    "        r.username,\n",
    "        COUNT(DISTINCT r.book_id) as libros_calificados,\n",
    "        COUNT(DISTINCT rev.review_id) as reviews_escritas\n",
    "    FROM ratings r\n",
    "    LEFT JOIN reviews rev ON r.username = rev.username\n",
    "    GROUP BY r.username\n",
    ")\n",
    "SELECT \n",
    "    CASE \n",
    "        WHEN libros_calificados >= 50 THEN 'POWER USERS (50+)'\n",
    "        WHEN libros_calificados >= 20 THEN 'ACTIVE USERS (20-49)'\n",
    "        WHEN libros_calificados >= 10 THEN 'REGULAR USERS (10-19)'\n",
    "        WHEN libros_calificados >= 5 THEN 'OCCASIONAL USERS (5-9)'\n",
    "        ELSE 'NOVICE USERS (1-4)'\n",
    "    END as segmento,\n",
    "    COUNT(*) as num_usuarios,\n",
    "    ROUND(AVG(libros_calificados), 1) as avg_libros,\n",
    "    ROUND(AVG(reviews_escritas), 1) as avg_reviews,\n",
    "    ROUND(SUM(reviews_escritas), 0) as total_reviews_segmento,\n",
    "    ROUND(AVG(reviews_escritas::DECIMAL / NULLIF(libros_calificados, 0)) * 100, 1) as engagement_ratio\n",
    "FROM user_stats\n",
    "GROUP BY segmento\n",
    "ORDER BY avg_libros DESC;\n",
    "\"\"\"\n",
    "\n",
    "user_data = pd.read_sql(user_query, engine)\n",
    "\n",
    "# Visualization: User Pyramid and Value Analysis\n",
    "fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(18, 8))\n",
    "fig.suptitle('üë• USER ENGAGEMENT HIERARCHY: The Power User Advantage', fontsize=18, fontweight='bold')\n",
    "\n",
    "# User Pyramid\n",
    "colors_pyramid = ['#FFD700', '#FF6B6B', '#4ECDC4', '#95E1D3', '#F38181']\n",
    "bars = ax1.bar(range(len(user_data)), user_data['num_usuarios'], \n",
    "               color=colors_pyramid, alpha=0.85, edgecolor='black')\n",
    "\n",
    "ax1.set_xticks(range(len(user_data)))\n",
    "ax1.set_xticklabels([seg.replace(' ', '\\n') for seg in user_data['segmento']], \n",
    "                    fontsize=10, fontweight='bold')\n",
    "ax1.set_title('üìä User Engagement Pyramid', fontsize=14, fontweight='bold')\n",
    "ax1.set_ylabel('Number of Users')\n",
    "\n",
    "# Add user counts\n",
    "for i, value in enumerate(user_data['num_usuarios']):\n",
    "    ax1.text(i, value + 15, str(value), ha='center', va='bottom', fontweight='bold')\n",
    "\n",
    "# Review Productivity Comparison\n",
    "productivity_segments = ['Power Users', 'Active Users', 'Average User']\n",
    "productivity_values = [\n",
    "    user_data[user_data['segmento'].str.contains('POWER')]['avg_reviews'].iloc[0],\n",
    "    user_data[user_data['segmento'].str.contains('ACTIVE')]['avg_reviews'].iloc[0],\n",
    "    user_data['avg_reviews'].mean()\n",
    "]\n",
    "\n",
    "bars2 = ax2.bar(productivity_segments, productivity_values, \n",
    "                color=['#FFD700', '#FF6B6B', '#CCCCCC'], alpha=0.85)\n",
    "\n",
    "ax2.set_title('‚ö° Review Productivity Comparison', fontsize=14, fontweight='bold')\n",
    "ax2.set_ylabel('Average Reviews per User')\n",
    "\n",
    "# Add productivity values\n",
    "for i, value in enumerate(productivity_values):\n",
    "    ax2.text(i, value + 0.5, f'{value:.1f}', ha='center', va='bottom', fontweight='bold')\n",
