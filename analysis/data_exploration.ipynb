    "## üëë Author Success Analysis\n",
    "\n",
    "**Commercial Potential Formula:** Rating √ó Popularity √ó Productivity √∑ 100"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Author Commercial Potential Analysis\n",
    "author_query = \"\"\"\n",
    "WITH author_metrics AS (\n",
    "    SELECT \n",
    "        a.author,\n",
    "        COUNT(DISTINCT b.book_id) as num_libros,\n",
    "        COUNT(DISTINCT r.rating_id) as total_ratings,\n",
    "        ROUND(AVG(r.rating), 2) as rating_promedio,\n",
    "        COUNT(DISTINCT rev.review_id) as total_reviews\n",
    "    FROM authors a\n",
    "    JOIN books b ON a.author_id = b.author_id\n",
    "    LEFT JOIN ratings r ON b.book_id = r.book_id\n",
    "    LEFT JOIN reviews rev ON b.book_id = rev.book_id\n",
    "    GROUP BY a.author_id, a.author\n",
    "    HAVING COUNT(DISTINCT r.rating_id) >= 30\n",
    ")\n",
    "SELECT \n",
    "    author,\n",
    "    num_libros,\n",
    "    total_ratings,\n",
    "    rating_promedio,\n",
    "    total_reviews,\n",
    "    ROUND((rating_promedio * total_ratings * num_libros) / 100, 1) as potencial_comercial,\n",
    "    CASE \n",
    "        WHEN num_libros <= 5 AND rating_promedio >= 4.2 THEN 'Saga Epic'\n",
    "        WHEN num_libros >= 10 AND total_ratings >= 100 THEN 'Volume Beast'\n",
    "        WHEN rating_promedio >= 4.0 AND num_libros < 8 THEN 'Emotional Pure'\n",
    "        WHEN rating_promedio >= 4.3 THEN 'Classic Timeless'\n",
    "        ELSE 'Balanced Strategy'\n",
    "    END as strategy_type\n",
    "FROM author_metrics\n",
    "ORDER BY potencial_comercial DESC\n",
    "LIMIT 10;\n",
    "\"\"\"\n",
    "\n",
    "author_data = pd.read_sql(author_query, engine)\n",
    "\n",
    "# Visualization: Author Success Matrix\n",
    "fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(18, 8))\n",
    "fig.suptitle('üëë AUTHOR SUCCESS MATRIX: Commercial Potential Leaders', fontsize=18, fontweight='bold')\n",
    "\n",
    "# Commercial Potential Ranking\n",
    "top_8 = author_data.head(8)\n",
    "colors_gradient = plt.cm.viridis(np.linspace(0, 1, len(top_8)))\n",
    "\n",
    "bars = ax1.barh(range(len(top_8)), top_8['potencial_comercial'], \n",
    "                color=colors_gradient, alpha=0.8)\n",
    "\n",
    "ax1.set_yticks(range(len(top_8)))\n",
    "ax1.set_yticklabels(top_8['author'], fontsize=11)\n",
    "ax1.set_title('üí∞ Commercial Potential Ranking', fontsize=14, fontweight='bold')\n",
    "ax1.set_xlabel('Commercial Potential Score')\n",
    "\n",
    "# Add values\n",
    "for i, value in enumerate(top_8['potencial_comercial']):\n",
    "    ax1.text(value + 1, i, f'{value}', va='center', fontweight='bold')\n",
    "\n",
    "# Strategy Distribution\n",
    "strategy_counts = author_data['strategy_type'].value_counts()\n",
    "colors_strategy = ['#FFD700', '#FF6B6B', '#4ECDC4', '#96CEB4', '#FECA57']\n",
    "\n",
    "wedges, texts, autotexts = ax2.pie(strategy_counts.values, \n",
    "                                   labels=strategy_counts.index,\n",
    "                                   colors=colors_strategy[:len(strategy_counts)],\n",
    "                                   autopct='%1.1f%%',\n",
    "                                   startangle=90)\n",
    "\n",
    "ax2.set_title('üéØ Success Strategy Distribution', fontsize=14, fontweight='bold')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Display top performers\n",
    "print(\"üèÜ TOP 5 AUTHORS BY COMMERCIAL POTENTIAL:\")\n",
    "print(\"=\" * 60)\n",
    "for idx, row in author_data.head(5).iterrows():\n",
    "    print(f\"üëë {row['author']}\")\n",
    "    print(f\"   Commercial Potential: {row['potencial_comercial']} points\")\n",
    "    print(f\"   Strategy: {row['strategy_type']}\")\n",
    "    print(f\"   Books: {row['num_libros']} | Rating: {row['rating_promedio']} | Ratings: {row['total_ratings']:,}\")\n",
    "    print()"
   ]
